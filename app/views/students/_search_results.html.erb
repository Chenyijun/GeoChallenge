<% if @nearbyStudents %>
<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Location</th>
      <th>City</th>
      <th>State</th>
      <th>Country</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @nearbyStudents.each do |student| %>
      <tr>
        <td><%= student.first_name %></td>
        <td><%= student.last_name %></td>
        <td><%= student.location %></td>
        <td><%= student.city %></td>
        <td><%= student.state %></td>
        <td><%= student.country %></td>
        <td><%= link_to 'Show', student %></td>
      </tr>
    <% end %>
<% end %>
  </tbody>
</table>